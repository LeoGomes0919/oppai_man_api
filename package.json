  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e .env.development -- tsx watch --inspect src/shared/infra/http/server.ts",
    "dev:docker": "dotenv -e .env -- tsx watch --inspect src/shared/infra/http/server.ts",
    "start": "node build/server.js",
    "build": "tsup build src --out-dir build",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev --preview-feature",
    "migrate:deploy": "prisma migrate deploy",
    "start:prod": "dotenv -e .env -- migrate:deploy && npm run build && npm run start",
    "compose:build": "docker compose up -d --build",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:seed": "dotenv -e .env.development -- tsx prisma/seed/index.ts"
  },
